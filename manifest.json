{
  "manifest_version": 2,
  "name": "HEY_FIND_ITEM",
  "description": "HEY_FIND_ITEM",
  "version": "1.2.0",
  "permissions": ["//www.costco.co.kr/"],
  "icons": {
    "96": "icon.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": ["//www.costco.co.kr/*"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": ["inject.js", "assets/tada.mp3"],
  "browser_action": {
    "default_title": "telegram",
    "default_popup": "popup.html"
  }
}
